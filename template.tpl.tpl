// Enter your template code here.
const logToConsole = require('logToConsole');
const callInWindow = require('callInWindow');
const makeTableMap = require('makeTableMap');
const copyFromDataLayer = require('copyFromDataLayer');
const queryPermission = require('queryPermission');
const getType = require('getType');

const addUserProperties = data.addUserProperties;
const addEventParams = data.addEventParams;
const event_name = data.eventName;
const debug = data.debug;
const eventType = data.eventType;

if(eventType == 'other'){
  // trigger the event, with or without custom event parameters
  if(addEventParams){
    const event_params = makeTableMap(data.eventParams, 'varName', 'valOutput');
    if(debug){
      logToConsole("logEvent: event name: ", event_name, 'params: ', event_params);
    }
    callInWindow('logEvent', event_name, event_params);
  }else{
    if(debug){
      logToConsole("logEvent: event name: ", event_name);
    }  
    callInWindow('logEvent', event_name);
  }
}

// here's the e-commerce magic
if(eventType == 'ecommerce'){
  let standardMapping = {};
  const keyPath = 'ecommerce.items';
  if (queryPermission('read_data_layer', keyPath)) {   
     standardMapping = {
      'add_payment_info': data.add_payment_info,
      'add_shipping_info': data.add_shipping_info,
      'add_to_cart': data.add_to_cart,
      'add_to_wishlist': data.add_to_wishlist,
      'begin_checkout': data.begin_checkout,
      'purchase': data.purchase,
      'refund': data.refund,
      'remove_from_cart': data.remove_from_cart,
      'select_item': data.select_item,
      'select_promotion': data.select_promotion,
      'view_cart': data.view_cart,
      'view_item': data.view_item,
      'view_item_list': data.view_item_list,
      'view_promotion': data.view_promotion
    };
    const ecom = data.inputSelectionVariable ? data.inputSelectionVariable : copyFromDataLayer('ecommerce', 1);
    const itemsArray = ecom.items;   
    
    if (getType(itemsArray) !== 'array') {
        if (debug) {
        logToConsole('no array of products available');
        return;
        }
    }
    
    if (getType(itemsArray) === 'array') {
      // Handle view_promotion and select_promotion events
      if (event_name === 'view_promotion' || event_name === 'select_promotion') {
        for (let i = 0; i < itemsArray.length; i++) {
          let promotion = itemsArray[i];
          let promotionEventParams = makeTableMap(data.eventParams, 'varName', 'valOutput');
          // Add all properties of the promotion object directly to promotionEventParams
          for (let key in promotion) {
            if (promotion.hasOwnProperty(key)) {
              promotionEventParams[key] = promotion[key];
            }
          }

          // Check if itemsArray includes an object with item_id
          if (promotion.hasOwnProperty('item_id')) {
            if (debug) {
              logToConsole('logEvent for promotion - including items array: ', itemsArray);
            }
            callInWindow('logEvent', event_name, { items: itemsArray });
          } else {
            if (addEventParams) {
              if (debug) {
                logToConsole('logEvent for promotion: ', event_name, 'params: ', promotionEventParams);
              }
              callInWindow('logEvent', event_name, promotionEventParams);
            } else {
              if (debug) {
                logToConsole('logEvent for promotion - no custom params: ', promotionEventParams);
              }
              callInWindow('logEvent', event_name, promotionEventParams);
            }
          }
        }
      } else {
        // Handle other e-commerce events as usual
        const event_params = makeTableMap(data.eventParams, 'varName', 'valOutput');
        event_params.items = itemsArray;
        
        if (addEventParams) {
          if (debug) {
            logToConsole('event_params: ', event_params);
          }
          callInWindow('logEvent', event_name, event_params);
        } else {
          if (debug) {
            logToConsole('event_params - no custom params: ', itemsArray);
          }
          callInWindow('logEvent', event_name, { items: itemsArray });
        }
      }
    }
  }
}

// add some user properties
if(addUserProperties) {
  var userProperties = data.userProperties || [];
  for (var i=0; i < userProperties.length; i++) {
    if(debug){
      logToConsole("setUserProperty: ", userProperties[i].key, ' - ', userProperties[i].value);
    }  
    callInWindow('setUserProperty', userProperties[i].key, userProperties[i].value);
  }
}
// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


